package com.in28minutes.learnspringframework.test;

import java.io.Serializable;

public class SpringBeanVsJavaBean {
	
	class Pojo{
		private String text;
		private int number;
		public String toString() {
			return text+ ":" + number;
		}
	}
	
	class JavaBean implements Serializable{
		public JavaBean() {
			
		}
		
		private String text;
		private int number;
		
		
	}
	
	

}
